# global
alias clr=clear
alias ld="ls -d */"
alias nvim="~/nvim-osx64/bin/nvim"
alias vi="nvim"
alias vim="nvim"
alias ..="cd .."
alias ...="cd ../.."
alias hosts="sudo $EDITOR /etc/hosts"

# tmux
alias ta="tmux -2 a"
alias ts="tmux new-session -s"

# git 
alias gs="git status"
alias ga="git add *"
alias gl="git pull"
alias gp="git push"
alias glp="git pull | git push"
alias gc='git commit -m"$1"'
alias gcm='git commit --amend'
alias gt="git log --tree"


